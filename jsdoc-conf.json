{
  "opts": {
    "readme": "./README.md",
    "destination": "./jsdocs",
    "tutorials": "./doc"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "include": ["src"]
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverlinks": false,
    "monospaceLinks": false
  },
  "recurseDepth": 10
}