{
  "opts": {
    "readme": "./README_jsdoc.md",
    "destination": "./docs",
    "tutorials": "./docmd"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "include": ["src"]
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverlinks": false,
    "monospaceLinks": false,
    "default": {
      "includeDate": false
    }
  },
  "recurseDepth": 10
}